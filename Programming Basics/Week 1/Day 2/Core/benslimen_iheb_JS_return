function hello() {
    console.log('hello');
}
hello();
console.log('Dojo'); // hello Dojo 

function hello() {
    console.log('hello');
    return 15;
}
var result = hello();
console.log('result is', result); //15,result is hello

function numPlus(num) {
    console.log('num is', num);
    return num+15;
}
var result = numPlus(3);
console.log('result is', result); // num15,



